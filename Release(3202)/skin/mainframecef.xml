<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Window size="1024,720" sizebox="4,4,6,6" mininfo="1024,720" caption="0,0,0,40" roundcorner="4,4">
	<Include source="default_config.xml" />
	<Include source="control_config.xml" />
	<VerticalLayout name="frame" bkcolor="#FFFFFFFF">
		<VerticalLayout height="36" comment="标题栏" bkcolor="#FFF4F5F6">
			<HorizontalLayout name="hly_title" height="36" >
				<HorizontalLayout width="14"/>
				<Label name="lbl_title" width="300" align="left" text="CefFrameTitle" textcolor="#FF999999" font="1"/>
				<HorizontalLayout>
				</HorizontalLayout>
				<Button name="btnrestore" width="30" height="36" tooltip="点击还原" normalimage="min_nor.png" hotimage="min_hovpre.png" pushedimage="min_hovpre.png" visible="false" />
				<Button name="btnmax" width="30" height="36" tooltip="点击最大化" normalimage="min_nor.png" hotimage="min_hovpre.png" pushedimage="min_hovpre.png" />
				<Button name="btnclose" width="30" height="36" tooltip="点击隐藏" normalimage="close_nor.png" hotimage="close_hovpre.png" pushedimage="close_hovpre.png"/>
				<HorizontalLayout width="3" bkcolor="#FFFFFF00"/>
			</HorizontalLayout>
		</VerticalLayout>
		<VerticalLayout height="5" bkcolor="#FFFFFFFF"/>
		<HorizontalLayout height="28" bkcolor="#FFFFFFFF">
			<VerticalLayout width="5"/>
			<Button name="btn_backup" text="后退" width="48" height="24" normalimage="login_nor.png" hotimage="login_hov.png" pushedimage="login_pre.png" font="1" textcolor="#FFFFFFFF" disabledtextcolor="#FFFFFFFF" />
			<VerticalLayout width="5" bkcolor="#FFFFFFFF" />
			<Button name="btn_forword" text="前进" width="48" height="24" normalimage="login_nor.png" hotimage="login_hov.png" pushedimage="login_pre.png" font="1" textcolor="#FFFFFFFF" disabledtextcolor="#FFFFFFFF" />
			<VerticalLayout width="5" bkcolor="#FFFFFFFF" />
			<Button name="btn_go" text="开始" width="48" height="24" normalimage="login_nor.png" hotimage="login_hov.png" pushedimage="login_pre.png" font="1" textcolor="#FFFFFFFF" disabledtextcolor="#FFFFFFFF" />
			<VerticalLayout width="5" bkcolor="#FFFFFFFF" />
			<Edit name="edt_path" bordervisible="true" bordersize="1" bordercolor="#FF0095e5" height="26" textcolor="#FF333333" bkcolor="#FFFFFFFF" vscrollbar="false" readonly="false" enabled="true" padding="24,0,0,0"/>
			<HorizontalLayout width="10" />
		</HorizontalLayout>
		<Container name="container" bkcolor="#FFFFFFFF">
		</Container>
		<VerticalLayout height="20" />
	</VerticalLayout>
</Window>
